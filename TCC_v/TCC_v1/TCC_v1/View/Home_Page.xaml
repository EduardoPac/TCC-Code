<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TCC_v1.View.Home_Page"
             Title="Forms">
    <StackLayout x:Name="stack">
        <ListView ItemsSource="{Binding Form}" x:Name="lvForm">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <ImageCell Text="{Binding DatasetName}" Detail="{Binding DataSetDescription}"/>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>

    </StackLayout>
    
    
</ContentPage>